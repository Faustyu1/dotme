    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "autoprefixer": "^10.4.13",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "13.4.19",
    "postcss": "^8.4.21",
    "prisma": "^5.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.2.2"
  }
