{
  "name": "dotme",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "dotenv -c -- prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:dev": "dotenv -c -- prisma migrate dev",
    "db:push": "dotenv -c -- prisma db push",
    "migrate:reset": "dotenv -c -- prisma migrate reset",
    "db:seed": "dotenv -c -- prisma db seed",
    "prisma:generate": "dotenv -c -- prisma generate",
    "prisma:studio": "dotenv -c -- prisma studio"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.14",
    "@prisma/client": "^5.2.0",
    "@solely/simple-fm": "^1.5.3",
    "date-fns": "^2.30.0",
    "next": "^13.4.19",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.7"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.13",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.49.0",
    "eslint-config-next": "13.4.19",
    "postcss": "^8.4.21",
    "prisma": "^5.2.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.2.2"
  }
}
