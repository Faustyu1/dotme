{
  "name": "dotme",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "dotenv -c -- prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:dev": "dotenv -c -- prisma migrate dev",
    "db:push": "dotenv -c -- prisma db push",
    "migrate:reset": "dotenv -c -- prisma migrate reset",
    "db:seed": "dotenv -c -- prisma db seed",
    "prisma:generate": "dotenv -c -- prisma generate",
    "prisma:studio": "dotenv -c -- prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.2.0",
    "@solely/simple-fm": "^1.5.3",
    "cva": "^1.0.0-beta.1",
    "date-fns": "^2.30.0",
    "lucide-react": "^0.276.0",
    "next": "^14.0.0",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^9.0.0",
    "rehype-slug": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "remark-toc": "^9.0.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^1.7.0",
    "@tailwindcss/typography": "^0.5.10",
